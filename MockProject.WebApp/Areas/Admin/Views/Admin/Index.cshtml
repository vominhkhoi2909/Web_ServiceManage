            })
            .catch(error => {
                notify('error', 'get dashboard have some problem!!')
            })

        // render Staff JO
        function renderStaffJobOrder(arr) {
            let html = arr.map((item, index) => {
                // set color of note type
                let colorType = '#a8afbf'
                if (item.type == 1) colorType = '#ffa7a7'
                if (item.type == 2) colorType = '#95d5f1'
                if (index >= 4) return ''
                return `
                                                      <li>
                                                            <div class="activity_bell" style="background: ${colorType};"></div>
                                                            <div class="timeLine_inner d-flex align-items-center">
                                                                <div class="activity_wrap">
                                                                    <h6>${item.createBy_Name} - ${moment(item.createAt).fromNow()}</h6>
                                                                    <p class="staff__joborder_des" data-toggle="tooltip" data-placement="top" title="Tooltip on top">
                                                                        ${item.description}
                                                                    </p>
                                                                </div>
                                                            </div>
                                                       </li>
                                                `
            }).join('')
            $('#staff__note_list').html(html)
        }

        function renderStaffJOFetch() {
            getData(`${BASE_API}api/Note/All`)
                .then(response => {
                    console.log(response)
                    renderStaffJobOrder(response.data)
                })
                .catch(error => {
                    notify('error', 'get Staff Job Order have some problem!!')
                })
        }
        renderStaffJOFetch()
    </script>
}
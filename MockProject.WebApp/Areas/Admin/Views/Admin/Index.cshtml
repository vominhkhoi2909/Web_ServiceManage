                    }, yaxis: { min: 0, max: 60, labels: { show: false, style: { fontSize: '12px', } } }, states: { normal: { filter: { type: 'none', value: 0 } }, hover: { filter: { type: 'none', value: 0 } }, active: { allowMultipleDataPointsSelection: false, filter: { type: 'none', value: 0 } } }, tooltip: { style: { fontSize: '12px', }, y: { formatter: function(val) { return "$" + val + " thousands" } } }, colors: ["#9767FD", "#E5ECFF"], markers: {
